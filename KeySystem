-- StarterGui.EnctonKeySystem.Main.StartupAnimation
local function C_18()
local script = G2L["18"];
	script.Parent.Size = UDim2.new(0, 390,0, 5)
	
	
	--startup tweening anim
	game:GetService("TweenService"):Create(
		script.Parent,
		TweenInfo.new(
			.7,
			Enum.EasingStyle.Sine,
			Enum.EasingDirection.Out,
			0,
			false,
			0
		),
		{Size = UDim2.new(0, 390,0, 299)}
		)
		:Play()
	
	wait(1)
	
end;
task.spawn(C_18);

return G2L["1"], require;
