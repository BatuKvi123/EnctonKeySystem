-- StarterGui.EnctonKeySystem.Main.ButtonLogic
local function C_d()
local script = G2L["d"];
	loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/RequiredFile_FolderCreator"))()
	
	--[[
	if not isfile("EnctonWorkspace/EnctonKeyInput") then
		writefile("EnctonWorkspace/EnctonKeyInput", "")
	end
	--]]
	-- KEY SYSTEM
	local KeyGuardLibrary = loadstring(game:HttpGet("https://cdn.keyguardian.org/library/v1.0.0.lua"))()
local trueData = "dfa1cfaaaeb447288b32f1083b61568a"
local falseData = "0a001a260bf44c7fa1b04a36c9c7a76a"

KeyGuardLibrary.Set({
  publicToken = "703ed6bd5c9e455c93500080d697bcd0",
  privateToken = "79dc2a4917f346fdabfa990f1b4032e0",
  trueData = trueData,
  falseData = falseData,
})

local key = script.Parent.KeyInput.Text

--copy key
script.Parent.CopyKey.MouseButton1Click:Connect(function()
setclipboard(KeyGuardLibrary.getLink())
end)

script.Parent.Check.MouseButton1Click:Connect(function()
KeyGuardLibrary.validateDefaultKey(key)
end)

if response == trueData then
  print("Key is valid")
else
  print("Key is invalid")
end

--[[
  KeyGuardLibrary.validateDefaultKey(key) - Validate key
  KeyGuardLibrary.validatePremiumKey(key) - Validate premium key
  KeyGuardLibrary.getService() - Get service
  KeyGuardLibrary.getLink() - Get link
]]
end;
task.spawn(C_d);

return G2L["1"], require;
