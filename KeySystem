-- StarterGui.EnctonKeySystem.Main.FreemiumKeyInput.LocalScript
local function C_7()
local script = G2L["7"];
	local p = "EnctonWorkspace/EnctonKeyInput.lua"
	
	script.Parent:GetPropertyChangedSignal("Text"):Connect(function()
		local text = script.Parent.Text
		writefile(p, text)
	end)
end;
task.spawn(C_7);
-- StarterGui.EnctonKeySystem.Main.ButtonLogic
local function C_10()
local script = G2L["10"];
	local link = "https://raw.githubusercontent.com/BatuKvi123/EnctonKeySystem/refs/heads/main/isKeyless.json"
	local response = http.request({
		Url = link,
		Method = "GET"
	})
	if response and response.StatusCode == 200 and response.Body then
		local HttpService = game:GetService("HttpService")
		local success, decoded = pcall(function()
			return HttpService:JSONDecode(response.Body)
		end)
		if success then
			local keyless = decoded.keyless
			if keyless == true then
				G2L["1"]:Destroy()
				loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/EnctonSource"))()
			else
				print("isKeyless is disabled")
			end
		end
	end
	
	
	local function wrongFreemium()
		script.Parent.CheckFreemium.Text = "Wrong!"
		game:GetService("TweenService"):Create(script.Parent.CheckFreemium, 
			TweenInfo.new(0.7, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0
			),
			{TextColor3 = Color3.new(0.776471, 0, 0)}
		):Play()
		wait(1)
		game:GetService("TweenService"):Create(script.Parent.CheckFreemium, 
			TweenInfo.new(0.7, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0
			),
			{TextColor3 = Color3.new(1, 1, 1)}
		):Play()
		wait(1)
		script.Parent.CheckFreemium.Text = "Check Freemium"
	end
	
	local function wrongPremium()
		script.Parent.CheckPremium.Text = "Wrong!"
		game:GetService("TweenService"):Create(script.Parent.CheckPremium, 
			TweenInfo.new(0.7, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0
			),
			{TextColor3 = Color3.new(0.776471, 0, 0)}
		):Play()
		wait(1)
		game:GetService("TweenService"):Create(script.Parent.CheckPremium, 
			TweenInfo.new(0.7, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0
			),
			{TextColor3 = Color3.new(1, 1, 1)}
		):Play()
		wait(1)
		script.Parent.CheckPremium.Text = "Check Premium"
	end
	
	
	
	
	
	
	
	
	
	
	local KeyGuardLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/nathzzi/Rift-Android/refs/heads/main/keyguardian/keyguardianlibrary.lua"))()
	local trueData = "a9ffc50d128f46afbbad2d521d136006"
	local falseData = "4d26ed71275344abad1ae43371933420"
	
	KeyGuardLibrary.Set({
		publicToken = "703ed6bd5c9e455c93500080d697bcd0",
		privateToken = "79dc2a4917f346fdabfa990f1b4032e0",
		trueData = trueData,
		falseData = falseData,
	})
	
	--Auto checking key for freemium users.
	local function autoCheckKeyFreemium()
		local response = KeyGuardLibrary.validateDefaultKey(readfile("EnctonWorkspace/EnctonKeyInput.lua")) or KeyGuardLibrary.validateDefaultKey(readfile("EnctonWorkspace/EnctonKeyInput"))
		if response == trueData then
			G2L["1"]:Destroy()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/EnctonSource"))()
		else
			wrongFreemium()
		end
	end
	
	--Auto checking key for premium users.
	local function autoCheckKeyPremium()
		local response = KeyGuardLibrary.validatePremiumKey(readfile("EnctonWorkspace/EnctonKeyInput.lua")) or KeyGuardLibrary.validateDefaultKey(readfile("EnctonWorkspace/EnctonKeyInput"))
		if response == trueData then
			G2L["1"]:Destroy()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/EnctonSource"))()
		else
			wrongPremium()
		end
	end
	
	local freemiumkey = script.Parent.FreemiumKeyInput
	local premiumkey = script.Parent.PremiumKeyInput
	
	script.Parent.CopyKey.MouseButton1Click:Connect(function()
	local getkey = KeyGuardLibrary.getLink()
		print("get your key from this link: "..getkey)
		setclipboard(getkey)
		end)
	
	script.Parent.CheckFreemium.MouseButton1Click:Connect(function()
		local response = KeyGuardLibrary.validateDefaultKey(freemiumkey.Text)
		print(response)
	
		if response == trueData then
			G2L["1"]:Destroy()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/EnctonSource"))()
		else
			wrongFreemium()
		end
	end)
	
	script.Parent.CheckPremium.MouseButton1Click:Connect(function()
		local response = KeyGuardLibrary.validatePremiumKey(premiumkey.Text)
		print(response)
	
		if response == trueData then
			G2L["1"]:Destroy()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/Encton/refs/heads/main/EnctonSource"))()
		else
			wrongPremium()
		end
	end)
	
	
	local readAutoCheckKey = readfile("EnctonSettings/AutoCheckKey.lua")
	if readAutoCheckKey == "Enabled" then
		task.spawn(autoCheckKeyFreemium)
		task.spawn(autoCheckKeyPremium)
	else
		print("no auto check enabled")
	end
	
	--[[
	  KeyGuardLibrary.validateDefaultKey(key) - Validate key
	  KeyGuardLibrary.validatePremiumKey(key) - Validate premium key
	  KeyGuardLibrary.getService() - Get service
	  KeyGuardLibrary.getLink() - Get link
	]]
end;
task.spawn(C_10);
